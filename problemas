//Problema 8.-

int main(){
	int suma=0, x;
	int numero;
	cout<<"De cuantos numeros sera la media? "<<endl;
	cin>>x;
	cout<<"Escriba los numeros:"<<endl;
	for(int i=0;i<x;i++){
		cin>>numero;
		suma+=numero;
	}
	float resultado=suma/x;
	cout<<"El resutado es: ";
	cout<<resultado;
	return 0;
}


